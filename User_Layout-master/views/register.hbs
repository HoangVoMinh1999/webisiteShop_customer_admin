<div class="limiter">
	<div class="container-login100">
		<div class="wrap-login100">
			<form class="login100-form validate-form" action="/register" method="POST">
				<span class="login100-form-title p-b-34">
					<h1 style="color: #363432;font-family:sans-serif"><span
							style="color:#FE980F;font-family: abel;">AEMÌNH</span>DUI</h1>
				</span>

				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="text" name="name" placeholder="Nhập tên người dùng"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="text" name="email" placeholder="Nhập email"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="text" name="phone" placeholder="Nhập số điện thoại"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="text" name="username" placeholder="Nhập Username"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="password" name="password" placeholder="Nhập mật khẩu"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100  validate-input m-b-20" data-validate="Type user name">
					<input id="first-name" class="input100" type="password" name="password_2" placeholder="Nhập lại mật khẩu"
						style="width: 100%;">
					<span class="focus-input100"></span>
				</div>
				<p style="color: red;">{{message}}</p>
				<div class="container-login100-form-btn">
					<button class="login100-form-btn" type="submit">
						Đăng ký
					</button>
				</div>

				<div class="w-full text-center p-t-27 p-b-239">
					<span class="txt1">
						Quên
					</span>

					<a href="#" class="txt2">
						Username/Mật khẩu
					</a>
				</div>
				<div class="w-full text-center">
					<a href="/login" class="txt3">
						Login
					</a>
					<a href="/" class="txt3">
						Trang Chủ
					</a>
				</div>
			</form>

			<div class="login100-more" style="background-image: url('login/images/bg-01.jpg');"></div>
		</div>
	</div>
</div>



<div id="dropDownSelect1"></div>